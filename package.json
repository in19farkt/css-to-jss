{
    "name": "css-to-jss",
    "displayName": "css-to-jss",
    "description": "For converting css rules to jss format",
    "version": "1.0.2",
    "publisher": "infarkt",
    "icon": "img/icon.png",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.cssToJss"
    ],
    "main": "./out/extension",
    "repository": {
        "type": "git",
        "url": "https://github.com/in19farkt/css-to-jss"
    },
    "contributes": {
        "commands": [
            {
                "command": "extension.cssToJss",
                "title": "CSS to JSS or JSS to CSS"
            }
        ],
        "keybindings": [
            {
                "command": "extension.cssToJss",
                "key": "Alt+x"
            }
        ],
        "configuration": {
            "title": "CSS to JSS configuration",
            "properties": {
                "css-to-jss.use-double-quotes": {
                    "type": "boolean",
                    "default": false,
                    "description": "Use double quote for property values"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
